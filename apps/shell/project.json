{
  "name": "shell",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/shell/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "apps/shell/dist",
        "index": "apps/shell/src/index.html",
        "baseHref": "/",
        "main": "apps/shell/src/main.tsx",
        "tsConfig": "apps/shell/tsconfig.app.json",
        "assets": [
          "apps/shell/src/favicon.ico",
          "apps/shell/src/assets"
        ],
        "styles": ["apps/shell/src/styles.css"],
        "scripts": [],
        "webpackConfig": "apps/shell/webpack.config.ts"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      },
      "dependsOn": ["^build", "typecheck"]
    },
    "serve": {
      "executor": "@nx/react:module-federation-dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "shell:build",
        "hmr": true,
        "port": 4200
      },
      "configurations": {
        "development": {
          "buildTarget": "shell:build:development"
        },
        "production": {
          "buildTarget": "shell:build:production",
          "hmr": false
        }
      },
      "dependsOn": ["typecheck"]
    },
    "serve-static": {
      "executor": "@nx/react:module-federation-static-server",
      "defaultConfiguration": "production",
      "options": {
        "serveTarget": "shell:serve"
      },
      "configurations": {
        "development": {
          "serveTarget": "shell:serve:development"
        },
        "production": {
          "serveTarget": "shell:serve:production"
        }
      }
    }
  }
}
